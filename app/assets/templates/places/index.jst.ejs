<div class="row">
<div class="col-md-6">

<script>
$(document).ready(function() 
    { 
        $("#myTable").tablesorter(); 
    } 
); 
</script>
</div>
<div class="col-md-6">
<table class="table table-hover table-bordered tablesorter">
<thead>
  <th>Title</th>
  <th>Monthly Rent</th>
  <th>Address</th>
</thead>
<tbody>
<% _(places.models).each(function(place) { %>
    <tr>
      <td class="text-capitalize col-md-3">
        <a href="#places/<%= place.escape("id") %>"><%= place.get("title") %></a></td>
      <td><%= place.get("price") %></td>
      <td><%= place.get("address") %></td>
    </tr>
<% }) %>
</tbody>
</table>
</div>
</div>  